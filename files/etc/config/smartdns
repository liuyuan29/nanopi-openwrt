
config smartdns
	option port '6053'
	option prefetch_domain '1'
	option cache_size '5000'
	option rr_ttl_min '300'
	option rr_ttl_max '3600'
	option coredump '0'
	option server_name 'china'
	option seconddns_server_group 'world'
	option force_aaaa_soa '0'
	option ipv6_server '0'
	option seconddns_port '5335'
	option serve_expired '1'
	option seconddns_no_speed_check '1'
	option seconddns_no_rule_soa '1'
	option seconddns_no_cache '1'
	option seconddns_tcp_server '0'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_dualstack_selection '0'
	option redirect 'none'
	option enabled '0'
	option dualstack_ip_selection '0'
	option seconddns_enabled '1'
	option tcp_server '0'
	list old_redirect 'none'
	list old_port '6053'
	list old_enabled '1'
